{
  "name": "bkrukowski/error-dumper",
  "description": "Pretty errors and exceptions in PHP.",
  "license": "MIT",
  "keywords": ["utility", "debug", "exceptions", "errors"],
  "authors": [
    {
      "name": "Bart≈Çomiej Krukowski",
      "email": "bartlomiej@krukowski.me"
    }
  ],
  "require": {
    "php": "^5.3 || ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.* || 5.*",
    "satooshi/php-coveralls": "1.*",
    "phpmd/phpmd": "@stable",
    "squizlabs/php_codesniffer": "@stable",
    "fabpot/php-cs-fixer": "@stable"
  },
  "suggest": {
    "symfony/var-dumper": "Pretty var_dump without xdebug."
  },
  "conflict": {
    "symfony/var-dumper": "<2.0 || >=4.0"
  },
  "autoload": {
    "psr-4": {
      "ErrorDumper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ErrorDumper\\Tests\\": "tests/"
    }
  }
}
